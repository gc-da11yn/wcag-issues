
<div class="pagedetails">
	<div class="row">
		<div class="col-sm-6 col-md-5 col-lg-4">
			<a href="{{ pagedetails[locale].reportAProblemURL }}" class="btn btn-default btn-center">{{ pagedetails[locale].reportAProblem }}</a></div>
		<div class="wb-share col-sm-4 col-md-3 col-sm-offset-2 col-md-offset-4 col-lg-offset-5" data-wb-share='{"lnkClass": "btn btn-default btn-block"}'></div>
	</div>
	<dl id="wb-dtmd">
		<dt>{{ pagedetails[locale].dateModified }}</dt>
		<dd><time datetime="{{ post.date | date('YYYY-MM-DD') }}">{{ post.item|date("YYYY-MM-DD", locale) }}</time></dd>
	</dl>
</div>




